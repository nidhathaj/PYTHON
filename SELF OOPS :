#NO ARG (SELF METHOD)
class simple:
    def add(self):
        return self.a + self.b
    def dis(self):
        print("rcb won the match")
s=simple()
s.a=10
s.b=20
print(s.add())
s.dis()

#WTH ARG (NO SELF)
class simple:
    def rectangle(self,length,breath):
        print(length*breath)
r=simple()
r.rectangle(3,6)
